  <patternset id="excluded.from.compilation.lucian-login">
    <patternset refid="excluded.from.module.lucian-login"/>
  </patternset>
  
  <path id="lucian-login.module.sourcepath">
    <dirset dir="${module.lucian-login.basedir}">
      <include name="src"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.lucian-login" depends="compile.module.lucian-login.production,compile.module.lucian-login.tests" description="Compile module lucian-login"/>
  
  <target name="compile.module.lucian-login.production" depends="register.custom.compilers,compile.module.lucian-core" description="Compile module lucian-login; production classes">
    <mkdir dir="${lucian-login.output.dir}"/>
    <javac2 destdir="${lucian-login.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.lucian-login}/javac">
      <compilerarg line="${compiler.args.lucian-login}"/>
      <bootclasspath refid="lucian-login.module.bootclasspath"/>
      <classpath refid="lucian-login.module.production.classpath"/>
      <src refid="lucian-login.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.lucian-login"/>
    </javac2>
    
    <copy todir="${lucian-login.output.dir}">
      <fileset dir="${module.lucian-login.basedir}/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.lucian-login.tests" depends="register.custom.compilers,compile.module.lucian-login.production" description="compile module lucian-login; test classes" unless="skip.tests"/>
  
  <target name="clean.module.lucian-login" description="cleanup module">
    <delete dir="${lucian-login.output.dir}"/>
    <delete dir="${lucian-login.testoutput.dir}"/>
  </target>
